{
  "name": "ssh2-exec",
  "version": "0.5.3",
  "description": "Transparent use of `child_process.exec` and `ssh2.prototype.exec`",
  "keywords": [
    "child process",
    "exec",
    "ssh2",
    "ssh"
  ],
  "homepage": "https://github.com/adaltas/node-ssh2-exec",
  "license": "BSD-3-Clause",
  "author": "David Worms <david@adaltas.com>",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [],
  "engines": {
    "node": ">=0.10.1"
  },
  "main": "./lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-ssh2-exec.git"
  },
  "devDependencies": {
    "coffeescript": "^2.4.1",
    "mocha": "~6.2.2",
    "should": "~13.2.3",
    "ssh2-they": "~2.0.4"
  },
  "optionalDependencies": {},
  "scripts": {
    "preversion": "grep '## Trunk' CHANGELOG.md && npm test",
    "version": "version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "build": "coffee -b -o lib src",
    "pretest": "coffee -b -o lib src",
    "test": "mocha test/*.coffee"
  },
  "dependencies": {}
}
